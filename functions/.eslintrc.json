{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "script",
    "max-len": [
      2,
      {
        "code": 100,
        "tabWidth": 4,
        "ignoreUrls": true
      }
    ],
    "require-jsdoc": false,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "always",
        "asyncArrow": "always"
      }
    ]
  },
  "extends": ["google"],
  "env": {
    "node": true
  }
}
